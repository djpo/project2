<h3>My collection</h3>

<% console.log('_____coming to you from summary.ejs_____'); %>

<% console.log(st); %>
<% console.log(typeof st); %>
<% console.log('---------------'); %>



<% if (st === 888) { %>
	<% console.log("if statement 1st path"); %>

	<div class="row center">
		<div id="soulTempoBox" class="soulTempoBoxEmpty">
			soul deprived...
		</div>
	</div>

<% } else if (st === 'NaN') { %>
	<% console.log("if statement 1.1"); %>
<% } else if (st === NaN) { %>
	<% console.log("if statement 1.2"); %>
<% } else if (st == NaN) { %>
	<% console.log("if statement 1.3"); %>
<% } else if (st == "NaN") { %>
	<% console.log("if statement 1.4"); %>
<% } else if (st === -1) { %>
	<% console.log("if statement 1.5"); %>


<% } else { %>
	<% console.log("if statement 2nd path"); %>

	<div class="row center">
		<div id="soulTempoBox">
			<%= st %>
		</div>
	</div>


	<div class="row labelRow">
		<div class="col-xs-1">
			tempo
		</div>
		<div class="col-xs-5">
			artist
		</div>
		<div class="col-xs-6">
			title
		</div>
	</div>

	<% collection.forEach(function(entry) { %>
		<div class="row songRow">
			<div class="col-xs-5">
				<%= entry.dataValues.artist %>
			</div>
			<div class="col-xs-6">
				<%= entry.dataValues.title %>
			</div>	
			<div class="col-xs-1">
				<%= Math.round(entry.dataValues.tempo); %>
			</div>
		</div>
	<% }); %>

<% } %>

